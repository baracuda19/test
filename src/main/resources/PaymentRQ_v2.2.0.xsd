<xs:schema xmlns="http://www.opentravel.org/OTA/2003/05/beta" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.opentravel.org/OTA/2003/05/beta" elementFormDefault="qualified" version="1.0.0" id="OTA2003B">
	<xs:simpleType name="ISO3166">
		<xs:annotation>
			<xs:documentation xml:lang="en">2 character country code as defined in ISO3166.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-zA-Z]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Money">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for amounts, max 3 decimals</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OTA_CodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Used for codes in the OTA code tables.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Z]{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="BalanceUsageGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Balance amount for each Usage type</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Usage" type="xs:string"/>
		<xs:attribute name="Balance" type="Money"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="CurrencyAmountGroup">
		<xs:annotation>
			<xs:documentation>Provides a monetary amount and the currency code to reflect the currency in which this amount is expressed.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Amount" type="Money" use="optional"/>
		<xs:attribute name="CurrencyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>3 alpha ISO 4217</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NumericCurrencyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>3 numeric ISO 4217</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DecimalPlaces" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the number of decimal places for a particular currency. This is equivalent to the ISO 4217 standard &quot;minor unit&quot;.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ExchangeRate" type="xs:double" use="optional">
			<xs:annotation>
				<xs:documentation>Rate of exchange when CurrencyCode is different than Account currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="CurrencyCodeGroup">
		<xs:annotation>
			<xs:documentation>Provides a currency code to reflect the currency in which an amount may be expressed.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="CurrencyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>3 alpha ISO 4217</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="NumericCurrencyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>3 numeric ISO 4217</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DecimalPlaces" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Indicates the number of decimal places for a particular currency. This is equivalent to the ISO 4217 standard &quot;minor unit&quot;.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="IssuerNameGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of bank or organization issuing the card (e.g., alumni association, bank, fraternal organization, etc.).</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BankName" type="xs:string" use="optional"/>
		<xs:attribute name="BankID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Code of bank issuing the card.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:attributeGroup name="PersonNameGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of a person.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Name" type="xs:string" use="optional"/>
		<xs:attribute name="FirstName" type="xs:string" use="optional"/>
		<xs:attribute name="LastName" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="OriginDestinationGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Origin and Destination City</xs:documentation>
		</xs:annotation>
		<xs:attribute name="OriginCity" type="xs:string" use="optional"/>
		<xs:attribute name="DestinationCity" type="xs:string" use="optional"/>
	</xs:attributeGroup>
	<xs:attributeGroup name="UserNameGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Username and Password</xs:documentation>
		</xs:annotation>
		<xs:attribute name="UserName" type="xs:string"/>
		<xs:attribute name="Password" type="xs:string"/>
	</xs:attributeGroup>
	<xs:complexType name="AddressType">
		<xs:sequence>
			<xs:element name="StreetNmbr" type="StreetNmbrType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Street Name and Number within the address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BldgRoom" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Building name, room, apartment, or suite number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AddressLine1" type="xs:string" minOccurs="0"/>
			<xs:element name="AddressLine2" type="xs:string" minOccurs="0"/>
			<xs:element name="CityName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">City name eg. Dublin</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PostalCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Post Office Code number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="County" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">County Name eg. Fairfax</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="StateProv" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="StateProvType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Country" type="CountryNameType" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Type" type="OTA_CodeType" use="optional">
			<xs:annotation>
				<xs:documentation>Defines the type of address (e.g. home, business, other). Refer to OTA Code List Communication Location Type (CLT).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="OTA_CodeType" use="optional">
			<xs:annotation>
				<xs:documentation>Can be used to further describe the address or location such as "Airport", "Downtown", etc.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AVS_ResultType">
		<xs:attribute name="AVS_ResultCode" type="xs:string" use="optional"/>
		<xs:attribute name="AVS_Remarks" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CarRentalType">
		<xs:sequence>
			<xs:element name="Pickup">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PickupAddress" type="AddressType"/>
					</xs:sequence>
					<xs:attribute name="PickupDateTime" type="xs:dateTime" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Return">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ReturnAddress" type="AddressType"/>
					</xs:sequence>
					<xs:attribute name="ReturnDateTime" type="xs:dateTime" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Renter">
				<xs:complexType>
					<xs:attributeGroup ref="PersonNameGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="ProviderInfo" type="ProviderInfoType"/>
			<xs:element name="Cost" type="CostType"/>
			<xs:element name="PaymentInfo" type="PaymentInfoType" minOccurs="0"/>
			<xs:element name="ServiceNote" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="AgreementNbr" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>Required by Amex (max 14 chars)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReferenceNbr" type="xs:string" use="required"/>
		<xs:attribute name="VehicleClassID" type="xs:string" use="optional"/>
		<xs:attribute name="Distance" type="xs:int" use="optional"/>
		<xs:attribute name="DistanceUnit" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="ContactInfoType">
		<xs:sequence>
			<xs:element name="EmailAddress" type="xs:string" minOccurs="0" maxOccurs="2"/>
			<xs:element name="PhoneNumber" type="PhoneType" minOccurs="0" maxOccurs="7"/>
		</xs:sequence>
		<xs:attributeGroup ref="PersonNameGroup"/>
	</xs:complexType>
	<xs:complexType name="CostType">
		<xs:annotation>
			<xs:documentation>Cost breakdown</xs:documentation>
		</xs:annotation>
		<xs:attribute name="CurrencyCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>3 alpha ISO 4217</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TotalAmount" type="Money" use="required"/>
		<xs:attribute name="BaseAmount" type="Money" use="optional"/>
		<xs:attribute name="Taxes" type="Money" use="optional"/>
		<xs:attribute name="Fees" type="Money" use="optional"/>
		<xs:attribute name="Commission" type="Money" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CountryNameType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The name or code of a country (e.g. as used in an address or to specify citizenship of a traveller).</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="Code" type="ISO3166" use="optional">
					<xs:annotation>
						<xs:documentation xml:lang="en">ISO 3166 code for a country.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Name" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>Country name e.g. Canada</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="CSC_ResultType">
		<xs:attribute name="CSC_ResultCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>CardSecurityCode verification result code:
I: Card verification number failed processor&apos;s
data validation check.
M: Card verification number matched.
N: Card verification number not matched.
P: Card verification number not processed.
S: Card verification number is on the card but
was not included in the request.
U: Card verification is not supported by the
issuing bank.
X: Card verification is not supported by the card
association.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CSC_Remarks" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="CurrencyAmountType">
		<xs:annotation>
			<xs:documentation>Provides a monetary amount and the code of the currency in which this amount is expressed.</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
	</xs:complexType>
	<xs:complexType name="DCCType">
		<xs:annotation>
			<xs:documentation>Dynamic Currency Conversion</xs:documentation>
		</xs:annotation>
		<xs:attribute name="DCC_TransID" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>SabreTransactionID as returned in DCC response</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OptedIn" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>"true" if cardholder opted to pay in card currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DocumentType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Document detail</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FOP_Amount" type="FOP_FareBreakType" minOccurs="0" maxOccurs="20"/>
			<xs:element name="TaxBreak" type="TaxType" minOccurs="0" maxOccurs="99"/>
			<xs:element name="MiscItem" type="MiscItemType" minOccurs="0" maxOccurs="99"/>
			<xs:element name="FareCalc" type="xs:string" minOccurs="0"/>
			<xs:element name="EMD_CouponDetail" minOccurs="0" maxOccurs="99">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Fare" type="FareBreakType" minOccurs="0"/>
						<xs:element name="TaxBreak" type="TaxType" minOccurs="0" maxOccurs="99"/>
						<xs:element name="Remarks" type="xs:string" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="RFIC_SubCode" type="xs:string" use="optional"/>
					<xs:attribute name="AssociatedDocNumber" type="xs:string" use="optional"/>
					<xs:attribute name="AssociatedSegmentNumber" type="xs:string" use="optional"/>
					<xs:attribute name="ConsumedAtIssuanceInd" type="xs:string" use="optional"/>
					<xs:attribute name="Quantity" type="xs:int" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DocType" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>TKT - Ticket
EMD 
MSR - Misc Sales Receipt
EXB - Excess Baggage
RFD - Refund, 
VCH - Voucher</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DocStatus" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>N - New (Default)
O - Original (the one being exchanged or refunded)</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="IssueLocation" type="xs:string" use="optional"/>
		<xs:attribute name="IssueDateTime" type="xs:dateTime" use="optional"/>
		<xs:attribute name="eTicketInd" type="xs:boolean" use="optional"/>
		<xs:attribute name="NetFareInd" type="xs:boolean" use="optional"/>
		<xs:attribute name="DocNumber" type="xs:string" use="optional"/>
		<xs:attribute name="CouponNumber" type="xs:string" use="optional"/>
		<xs:attribute name="BaseFare" type="Money" use="optional"/>
		<xs:attribute name="NetFare" type="Money" use="optional"/>
		<xs:attribute name="Taxes" type="Money" use="optional"/>
		<xs:attribute name="Fees" type="Money" use="optional"/>
		<xs:attribute name="Points" type="xs:integer" use="optional"/>
		<xs:attribute name="CategoryCode" type="xs:string" use="optional"/>
		<xs:attribute name="FlightDetailIndex" type="xs:integer" use="optional"/>
		<xs:attribute name="InitialDocNumber" type="xs:string" use="optional"/>
		<xs:attribute name="InitialDocType" type="xs:string" use="required"/>
		<xs:attribute name="AssociatedDocNumber" type="xs:string" use="optional"/>
		<xs:attribute name="DocSubType" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>For EMD, this could be A or S.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RFI_Code" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="FareBreakType">
		<xs:annotation>
			<xs:documentation>Provides a monetary amount and the code of the currency in which this amount is expressed.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BaseFare" type="Money" use="optional"/>
		<xs:attribute name="Taxes" type="Money" use="optional"/>
		<xs:attribute name="Fees" type="Money" use="optional"/>
		<xs:attribute name="CurrencyCode" type="xs:string" use="optional"/>
		<xs:attribute name="EquivalentBaseFare" type="Money" use="optional"/>
		<xs:attribute name="EquivalentCurrencyCode" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="FlightDetailType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Flight detail</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirlineCode" type="xs:string" minOccurs="0"/>
			<xs:element name="OperatingAirlineCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For codeshare flight.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FlightNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>1 - 4 digits flight number or OPEN or ARNK</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClassOfService" type="xs:string" minOccurs="0"/>
			<xs:element name="DepartureInfo" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="DepartureAirport" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>3-alpha IATA airport city code.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DepartureDateTime" type="xs:dateTime" use="required"/>
					<xs:attribute name="CurrentLocalDateTime" type="xs:dateTime" use="optional">
						<xs:annotation>
							<xs:documentation>Current local date/time of the departure city.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ArrivalInfo" minOccurs="0">
				<xs:complexType>
					<xs:attribute name="ArrivalAirport" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation>3-alpha IATA airport city code.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ArrivalDateTime" type="xs:dateTime" use="optional"/>
					<xs:attribute name="FinalDestinationInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>Indicates if the arrival city is the final destination.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DocNumber" type="xs:string" use="optional"/>
		<xs:attribute name="FareBasisCode" type="xs:string" use="optional"/>
		<xs:attribute name="TicketDesignator" type="xs:string" use="optional"/>
		<xs:attribute name="ReturnFlightInd" type="xs:boolean" use="optional"/>
		<xs:attribute name="PNR_SegmentNbr" type="xs:integer" use="optional"/>
		<xs:attribute name="ConnectingFlightInd" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>'X' - Transit, 'O' - Stopover</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CouponUseIndicator" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>'F' - Flight, 'S' - Surface</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="FOP_FareBreakType">
		<xs:annotation>
			<xs:documentation>Farebreak at FOP level</xs:documentation>
		</xs:annotation>
		<xs:attribute name="FOP_Type" type="xs:string" use="required"/>
		<xs:attribute name="FOP_Code" type="xs:string" use="optional"/>
		<xs:attribute name="CardNbr" type="xs:string" use="optional"/>
		<xs:attribute name="TokenizedCardNbr" type="xs:string" use="optional"/>
		<xs:attribute name="ExpireDate" type="xs:string" use="optional"/>
		<xs:attribute name="ApprovalCode" type="xs:string"/>
		<xs:attribute name="FOP_TotalAmt" type="Money" use="required"/>
		<xs:attribute name="BaseFare" type="Money" use="optional"/>
		<xs:attribute name="Taxes" type="Money" use="optional"/>
		<xs:attribute name="Fees" type="Money" use="optional"/>
		<xs:attribute name="PaymentCurrencyCode" type="xs:string" use="optional"/>
		<xs:attribute name="PaymentCurrencyAmt" type="Money" use="optional"/>
	</xs:complexType>
	<xs:complexType name="GuestType">
		<xs:attribute name="AgeQualifyingCode" type="OTA_CodeType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A code representing a business rule that determines the charges for a guest based upon age rannge (e.g. Adult, Child, Senior, Child With Adult, Child Without Adult). This attribute allows for an increase in rate by occupant class. Refer to OTA Code List Age Qualifying Code (AQC).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="VIP" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Guest is VIP indicator. </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PrimaryIndicator" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When true indicates this is the primary guest.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="PersonNameGroup"/>
	</xs:complexType>
	<xs:complexType name="HotelType">
		<xs:sequence>
			<xs:element name="ProviderInfo" type="ProviderInfoType"/>
			<xs:element name="Room" type="RoomStayType" minOccurs="0" maxOccurs="5"/>
			<xs:element name="Cost" type="CostType"/>
			<xs:element name="PaymentInfo" type="PaymentInfoType" minOccurs="0"/>
			<xs:element name="ServiceNote" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="AgreementNbr" type="xs:string" use="required"/>
		<xs:attribute name="LodgingType" type="xs:string" use="optional"/>
		<xs:attribute name="CheckInCity" type="xs:string" use="optional"/>
		<xs:attribute name="CheckInDate" type="xs:date" use="required"/>
		<xs:attribute name="CheckOutDate" type="xs:date" use="required"/>
		<xs:attribute name="ReferenceNbr" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation>External Voucher Number for EY</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="IssuerNameType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Name of bank or organization issuing the card (e.g., alumni association, bank, fraternal organization, etc.).</xs:documentation>
		</xs:annotation>
		<xs:attribute name="BankID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Code of bank issuing the card.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="MiscItemType">
		<xs:annotation>
			<xs:documentation>Misc doc detail</xs:documentation>
		</xs:annotation>
		<xs:attribute name="CategoryCode" type="xs:string" use="required"/>
		<xs:attribute name="UnitPrice" type="Money" use="required"/>
		<xs:attribute name="Quantity" type="xs:int" use="required"/>
		<xs:attribute name="Description" type="xs:string" use="optional"/>
	</xs:complexType>
	<xs:complexType name="PassengerDetailType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Passenger detail</xs:documentation>
		</xs:annotation>
