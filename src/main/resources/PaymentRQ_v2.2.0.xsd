	<xs:complexType name="POS_Type">
		<xs:annotation>
			<xs:documentation>Point of Sales information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BrowserDetail" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="HttpHeaders" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="HttpHeader" maxOccurs="unbounded">
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="xs:string">
													<xs:attribute name="Name" type="xs:string" use="required"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="BrowserSessionID" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserHostName" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserAccept" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserAcceptEncoding" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserAcceptCharset" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserID" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserLanguageCode" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserCookie" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserConnectionType" type="xs:string" use="optional"/>
					<xs:attribute name="BrowserReferer" type="xs:string" use="optional"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Javascript" type="xs:string" minOccurs="0"/>
			<xs:element name="POS_Address" type="AddressType" minOccurs="0"/>
